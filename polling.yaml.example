---
sources:
    - name: all_pollsters
      interval: 600
      meters:
          - "*"
    - name: meter_snmp
      interval: 600
      resources:
          - snmp://compute01
          - snmp://compute02
          - snmp://compute03
      meters:
          - "hardware.cpu*"
          - "hardware.memory*"
          - "hardware.disk*"
          - "hardware.network*"

